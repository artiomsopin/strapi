{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "inputZone": {
      "type": "dynamiczone",
      "components": [
        "form.phone",
        "form.name",
        "form.email"
      ],
      "required": true
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "minLength": 3
    },
    "registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::registration.registration",
      "private": true,
      "mappedBy": "page"
    }
  }
}
